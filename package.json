{
  "name": "@omoikane/template",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "node --env-file=.env dist/server/index.js",
    "db:create": "NODE_OPTIONS='--import tsx/esm --env-file=.env' mikro-orm schema:create --run",
    "db:migrate": "NODE_OPTIONS='--import tsx/esm --env-file=.env' mikro-orm migration:up",
    "db:migrate:create": "NODE_OPTIONS='--import tsx/esm --env-file=.env' mikro-orm migration:create",
    "db:seed": "NODE_OPTIONS='--import tsx/esm --env-file=.env' mikro-orm seeder:run",
    "test": "vitest",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@fastify/multipart": "^8.0.0",
    "@fastify/oauth2": "^7.0.0",
    "@fastify/secure-session": "^7.0.0",
    "@fastify/websocket": "^10.0.0",
    "@mikro-orm/core": "^6.0.0",
    "@mikro-orm/migrations": "^6.0.0",
    "@mikro-orm/postgresql": "^6.0.0",
    "@mikro-orm/reflection": "^6.0.0",
    "@mikro-orm/seeder": "^6.0.0",
    "@react-email/components": "0.0.12",
    "@react-email/render": "0.0.10",
    "argon2": "^0.40.0",
    "bullmq": "^5.0.0",
    "clsx": "^2.1.1",
    "fastify": "^4.0.0",
    "ioredis": "^5.0.0",
    "jotai": "^2.6.0",
    "nodemailer": "^7.0.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@elrond25/omoikane-eslint-config": "1.0.0",
    "@elrond25/vite-plugin-omoikane": "0.0.1",
    "@mikro-orm/cli": "^6.0.0",
    "@playwright/test": "^1.40.0",
    "@storybook/react": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@tailwindcss/typography": "^0.5.0",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.0",
    "storybook": "^10.1.11",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.3.1",
    "vitest": "^2.0.0"
  },
  "mikro-orm": {
    "configPaths": [
      "./src/server/config/mikro-orm.config.ts",
      "./dist/server/config/mikro-orm.config.js"
    ]
  }
}
